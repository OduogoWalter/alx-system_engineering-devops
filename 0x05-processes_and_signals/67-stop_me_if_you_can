#!/usr/bin/env bash
# 67-stop_me_if_you_can script stops the 7-highlander process

# Find and kill the 7-highlander process
pid=$(pgrep -f "7-highlander")
if [ -n "$pid" ]; then
    kill -15 "$pid"
    echo "Killed process 7-highlander with PID $pid"
else
    echo "Process 7-highlander not found"
fi
